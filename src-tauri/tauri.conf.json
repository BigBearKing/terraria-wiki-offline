{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Terraria Wiki",
  "version": "0.1.0",
  "identifier": "com.bbk.terraria-wiki",
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Terraria Wiki",
        "width": 800,
        "height": 600,
          "url": "wiki://localhost/index.html"
        }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico"
    ],
    "resources": [
      "../wiki-assets/"
    ]
  }
}
